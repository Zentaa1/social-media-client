{
  "name": "css-frameworks-ca",
  "version": "1.2.1",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install",
    "test": "npm run test-unit & npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-report": "cypress run --reporter mochawesome",
    "test-e2e-cli": "cypress run"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@eslint/js": "^9.0.0",
    "cypress": "^13.7.3",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^28.2.0",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "jest": "^29.2.0",
    "lint-staged": "^15.2.2",
    "live-server": "^1.1.0",
    "prettier": "latest",
    "sass": "latest"
  },
  "dependencies": {
    "bootstrap-dark-5": "latest",
    "dotenv": "^16.4.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}